# .env.test - Environment variables for E2E tests

# API URL - Where the test will send requests
API_URL=http://localhost:3000

# Test control flags
START_SERVICES=true
START_REDIS=true
START_APP=true

# Service configuration
PORT=3000
REDIS_HOST=localhost
REDIS_PORT=6379
NODE_ENV=test

# Rate limiter configuration - consider using lower values for faster testing
RATE_LIMIT_WINDOW_SIZE_SECONDS=60
RATE_LIMIT_STANDARD=100  # Lower for faster tests
RATE_LIMIT_HIGH=200      # Lower for faster tests
